<UserControl
	x:Class="IMTE.Views.MDLookup_ItemDescriptionConfig"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
	xmlns:local="clr-namespace:IMTE.Views"
	xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:prism="http://prismlibrary.com/"
	Width="900"
	Height="600"
	prism:ViewModelLocator.AutoWireViewModel="True"
	Background="White"
	mc:Ignorable="d">
	<GroupBox Header="Item Description Configuration">
		<Grid>
			<ContentControl
				x:Name="empConfigMainRegion"
				Grid.Row="2"
				Grid.ColumnSpan="2"
				prism:RegionManager.RegionName="ItemDescriptionRegionMain" />
		</Grid>
	</GroupBox>
</UserControl>
